{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "ESNext",
    "declaration": true,
    "outDir": "lib",
    "strict": true,
    "esModuleInterop": true,
    "pretty": true,
    "resolveJsonModule": true,
    "moduleResolution":"Node",
    "noImplicitThis": false,
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "paths": {},
  "include": [
    "package.json"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}